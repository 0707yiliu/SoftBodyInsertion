<mujoco model="soft body">
  <extension>
    <plugin plugin="mujoco.elasticity.solid"/>
  </extension>

<!--  <option solver="CG" tolerance="1e-6" timestep=".001"/>-->

  <size memory="10M"/>

  <visual>
    <map stiffness="100"/>
  </visual>

  <worldbody>
    <composite type="particle" count="2 2 2" spacing="0.03" offset="0.5 0.5 1">
      <geom size=".025" rgba=".8 .2 .1 1" group="4"/>
      <skin inflate="0" rgba="0 .7 .7 .6"/>
      <plugin plugin="mujoco.elasticity.solid">
        <config key="poisson" value="0"/>
        <config key="damping" value="0.001"/>
        <!--Units are in Pa (SI)-->
        <config key="young" value="5e4"/>
      </plugin>
    </composite>
  </worldbody>
</mujoco>
