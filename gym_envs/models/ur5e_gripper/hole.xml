<mujoco model="curved hole">
    <asset>
        <mesh name="tube1" file="cube_hole_1.stl" />
        <mesh name="tube2" file="cube_hole_2.stl" />
        <mesh name="tube3" file="cube_hole_3.stl" />
        <mesh name="tube4" file="cube_hole_4.stl" />
