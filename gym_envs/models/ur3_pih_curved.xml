<mujoco model="ur3e gripper">
    <statistic center="0.3 0 0.4" extent="0.8"/>
    <compiler angle="radian" meshdir="meshs/visual/" autolimits="true"/>
    <option>
        <flag gravity="disable" 
              warmstart="disable"/>
    </option>
    <option timestep="0.001" solver="CG" jacobian="sparse" impratio="10" tolerance="1e-10"/>
    <size njmax="8000" nconmax="4000"/>
    <visual>
        <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0 0 0"/>
        <rgba haze="0.15 0.25 0.35 1"/>
        <global azimuth="120" elevation="-20"/>
    </visual>
    <include file="world.xml"/>
    <include file="ur3_with_gripper.xml"/>
    <include file="hole.xml"/>
</mujoco>